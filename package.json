  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.1",
    "@types/debug": "^4.1.0",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/mongodb": "^3.1.19",
    "@types/mongoose": "^5.3.12",
    "@types/morgan": "^1.7.35",
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.10",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3"
